<div class='container-fluid'>
  <div class='row-fluid'>
    <%= render 'nav_list' %>
    <div class='span9'>
      <ul class="nav nav-pills">
        <li class="active">
        <a href='#'>
          chats
        </a>
        </li>
        <li class=''>
        <a href='/admin/robots/wikis'>
          wikis
        </a>
        </li>
      </ul>
      <div style='text-align:center;height:40px;'>
        <%= will_paginate @history, :class=>''%>
      </div>
      <div style='width:96%'>
        <a href='/admin/robots/clear' style='float:right;color:red;' data-confirm='are you sure?'>
          <i class='icon-trash'>
          </i>
          clear&nbsp;&nbsp;&nbsp;&nbsp;
        </a>
      </div>
      <br/>
      <br/>
      <table class='table' style='width:96%;'>
        <tr>
          <th>id</th>
          <th style='width:130px;'>time</th>
          <th>name</th>
          <th style='width:230px;'>receive</th>
          <th style='width:230px;'>reply</th>
          <th style='width:50px;'>edit</th>
        </tr>
        <% @history.each do |h| %>
          <tr>
            <td><%= h.id %></td>
            <td><%= h.created_at.strftime "%Y-%m-%d %H:%M:%S"%></td>
            <td><%= h.username %></td>
            <td><%= h.receive %></td>
            <td><%= h.reply.html_safe %></td>
            <td><a href="/admin/robots/delete/<%=h.id%>" data-remote=true class='history-delete'><i class='icon-trash' ></i>delete</a></td>
          </tr>
        <% end %>
      </table>
      <div style='text-align:center;height:40px;'>
        <%= will_paginate @history, :class=>''%>
      </div>
    </div>
  </div>
</div>
